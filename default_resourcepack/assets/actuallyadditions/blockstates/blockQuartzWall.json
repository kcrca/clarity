    {   "when": { "up": "true" },
      "apply": { "model": "actuallyadditions:blockQuartzWallPost" }
    },
    {   "when": { "north": "true" },
      "apply": { "model": "actuallyadditions:blockQuartzWallSide", "uvlock": true }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "actuallyadditions:blockQuartzWallSide", "y": 90, "uvlock": true }
