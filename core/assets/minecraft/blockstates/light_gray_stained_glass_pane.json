{
    "multipart": [
        {   "when": { "north": false, "south": false, "east": false, "west": false  },
	    "apply": { "model": "block/light_gray_stained_glass_pane_post" }
	},

        {   "when": { "north": true, "south": false, "east": false, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_side" }
        },
        {   "when": { "north": false, "south": false, "east": true, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_side", "y": 90 }
        },
        {   "when": { "north": false, "south": true, "east": false, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_side", "y": 180 }
        },
        {   "when": { "north": false, "south": false, "east": false, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_side", "y": 270 }
        },

        {   "when": { "north": true, "south": true, "east": false, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_across" }
        },
        {   "when": { "north": false, "south": false, "east": true, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_across", "y": 90 }
        },

        {   "when": { "north": true, "south": false, "east": false, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_bend" }
        },
        {   "when": { "north": true, "south": false, "east": true, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_bend", "y": 90 }
        },
        {   "when": { "north": false, "south": true, "east": true, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_bend", "y": 180 }
        },
        {   "when": { "north": false, "south": true, "east": false, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_bend", "y": 270 }
        },

        {   "when": { "north": true, "south": true, "east": false, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_tee" }
        },
        {   "when": { "north": true, "south": false, "east": true, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_tee", "y": 90 }
        },
        {   "when": { "north": true, "south": true, "east": true, "west": false  },
            "apply": { "model": "block/light_gray_stained_glass_pane_tee", "y": 180 }
        },
        {   "when": { "north": false, "south": true, "east": true, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_tee", "y": 270 }
        },

        {   "when": { "north": true, "south": true, "east": true, "west": true  },
            "apply": { "model": "block/light_gray_stained_glass_pane_cross" }
        }
    ]
}
